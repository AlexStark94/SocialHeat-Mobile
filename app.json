{
  "expo": {
    "name": "social-heat-mobile",
    "slug": "social-heat-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyA1Bym__8QKgrUnUul4-2ZydGaTaU-smJs"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleMapsApiKey": "AIzaSyA1Bym__8QKgrUnUul4-2ZydGaTaU-smJs",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.socialheat.socialheatmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SocialHeat to access your camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6a4150a7-7883-4ed0-a59e-19fad8838d0e"
      }
    }
  }
}
